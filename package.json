{
  "name": "tom-tom-manager",
  "version": "1.0.0",
  "description": "Best way to Manage TomTom at Home",
  "author": "miniBear",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && cd frontend && yarn install --dev && yarn build",
    "format": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "start": "yarn prebuild && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:staging": "node dist/main",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/app/test/jest-e2e.json",
    "db:create": "node ./create-db-script.js",
    "db:drop": "node ./drop-db-script.js",
    "migration:run": "npx typeorm migration:run",
    "migration:create": "npx typeorm migration:create -d ./src/db/migrations -n ",
    "migration:generate": "npx typeorm migration:generate -d ./src/db/migrations -n ",
    "migration:revert": "npx typeorm migration:revert",
    "console": "node dist/console.js"
  },
  "dependencies": {
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.4",
    "@types/bcrypt": "^3.0.1",
    "@types/chance": "^1.1.1",
    "bcrypt": "^5.0.0",
    "bull": "^3.20.0",
    "bull-board": "^1.2.0",
    "bullmq": "^1.12.2",
    "chance": "^1.1.7",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.12.2",
    "commander": "^6.2.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.9.4",
    "dotenv": "^8.2.0",
    "es6-shim": "^0.35.6",
    "express-basic-auth": "^1.2.0",
    "form-data": "^4.0.0",
    "http-proxy-middleware": "^1.3.1",
    "humps": "^2.0.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.20",
    "nestjs-console": "^4.0.0",
    "nestjs-pino": "^1.3.0",
    "nestjs-typeorm-paginate": "^2.3.2",
    "numeral": "^2.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.2",
    "pg-promise": "^10.9.4",
    "pino-pretty": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sharp": "^0.26.2",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.29",
    "typeorm-transactional-tests": "^1.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bull": "^3.14.4",
    "@types/cors": "^2.8.10",
    "@types/cron": "^1.7.2",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "^13.9.1",
    "@types/sharp": "^0.26.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^6.0.0",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
