{
  "name": "tom-tom-manager",
  "version": "1.3.1",
  "description": "Best way to Manage TomTom at Home",
  "author": "miniBear",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x",
    "yarn": "1.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:s": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "start": "yarn prebuild && nest start --watch",
    "start:c": "cd frontend && yarn start",
    "start:debug": "nest start --debug --watch",
    "start:staging": "node dist/main",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/app/test/jest-e2e.json",
    "db:create": "node ./create-db-script.js",
    "db:drop": "node ./drop-db-script.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:cmd:run": "yarn typeorm migration:run -d ./src/config/db-config.ts",
    "migration:cmd:create": "yarn typeorm -d ./src/config/db-config.ts migration:create",
    "migration:cmd:generate": "yarn typeorm -d ./src/config/db-config.ts migration:generate",
    "migration:cmd:revert": "yarn typeorm -d ./src/config/db-config.ts migration:revert",
    "migration:run": "env-cmd yarn migration:cmd:run",
    "migration:create": "env-cmd yarn migration:cmd:create",
    "migration:generate": "env-cmd yarn migration:cmd:generate",
    "migration:revert": "env-cmd yarn migration:cmd:revert",
    "console": "node dist/console.js",
    "semantic-release": "env-cmd -f .env semantic-release --no-ci"
  },
  "dependencies": {
    "@bull-board/express": "^4.11.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/bullmq": "^1.0.0",
    "@nestjs/common": "^9.3.7",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.2",
    "@nestjs/platform-express": "^9.3.7",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chance": "^1.1.1",
    "argon2": "^0.30.3",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.3",
    "bullmq": "^3.6.3",
    "cache-manager": "^5.1.6",
    "chance": "^1.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^6.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.2.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "es6-shim": "^0.35.6",
    "express-basic-auth": "^1.2.1",
    "form-data": "^4.0.0",
    "http-proxy-middleware": "^1.3.1",
    "humps": "^2.0.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.20",
    "nestjs-console": "^8.0.0",
    "nestjs-pino": "^3.1.2",
    "nestjs-typeorm-paginate": "^4.0.3",
    "numeral": "^2.0.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "pg-promise": "^11.2.0",
    "pino": "^8.9.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "semver": "^7.3.8",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.12",
    "typeorm-transactional-tests": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.7",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "conventional-changelog-eslint": "^3.0.9",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "26.4.2",
    "prettier": "^2.8.4",
    "semantic-release": "^20.1.0",
    "supertest": "^6.0.0",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepare": "echo ${nextRelease.version} > VERSION"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json",
            "yarn.lock"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
